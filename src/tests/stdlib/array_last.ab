import { last } from "std/array"

// Output
// Last of data: "date" (4) [apple banana cherry date]
// Last of empty: "" (0) []

fun test_last(label: Text, inner: [Text]): Null {
    let value = last(inner)
    echo "Last of {label}: \"{value}\" ({len(inner)}) [{inner}]"
}

main {
    let data = ["apple", "banana", "cherry", "date"]
    let empty = [Text]
    test_last("data", data)
    test_last("empty", empty)
}
