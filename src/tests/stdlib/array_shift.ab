import { shift } from "std/array"

// Output
// Shifted from data: "zero" (3) [one two three]
// Shifted from empty: "" (0) []

fun test_shift(label: Text, inner: [Text]): Null {
    let value = shift(inner)
    echo "Shifted from {label}: \"{value}\" ({len(inner)}) [{inner}]"
}

main {
    let data = ["zero", "one", "two", "three"]
    let empty = [Text]
    test_shift("data", data)
    test_shift("empty", empty)
}
