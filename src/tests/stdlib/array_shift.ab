import { shift } from "std/array"

// Output
// Shifted from data: "apple", [banana cherry date]
// Shifted from empty: "", []

fun test_shift(label: Text, inner: [Text]): Null {
    let value = shift(inner)
    echo "Shifted from {label}: \"{value}\", [{inner}]"
}

main {
    let data = ["apple", "banana", "cherry", "date"]
    let empty = [Text]
    test_shift("data", data)
    test_shift("empty", empty)
}
