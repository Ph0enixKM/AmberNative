import { pop } from "std/array"

// Output
// Popped from data: "date", [apple banana cherry]
// Popped from empty: "", []

fun test_pop(label: Text, inner: [Text]): Null {
    let value = pop(inner)
    echo "Popped from {label}: \"{value}\", [{inner}]"
}

main {
    let data = ["apple", "banana", "cherry", "date"]
    let empty = [Text]
    test_pop("data", data)
    test_pop("empty", empty)
}
